{
    "name": "you-are-awesome-app-serverless-backend",
    "version": "0.3.0",
    "author": "Mirco Kraenz",
    "license": "UNLICENSED",
    "scripts": {
        "build": "tsc",
        "clean": "del-cli dist .build",
        "predeploy": "npm run clean",
        "deploy": "serverless deploy",
        "deploy:fn": "npm run clean && serverless deploy function -f",
        "deploy:test": "serverless offline",
        "deploy:test:verbose": "serverless offline --printOutput",
        "deploy:test:send-push-notif": "serverless offline --config serverless.pre.yml",
        "dev": "npm run deploy:test",
        "test": "docker-compose up -d && jest",
        "test:watch": "docker-compose up -d && jest --watch",
        "invoke:send-push-notif": "aws lambda invoke --function-name y3a-prod-sendPushNotif --payload '{\"time\": \"2021-08-27T17:12:07.000Z\"}' --cli-binary-format raw-in-base64-out --region eu-central-1 --profile typescriptteatime response.json",
        "invoke:handle-success-tickets": "aws lambda invoke --function-name y3a-prod-handlePushNotifSuccessTickets --region eu-central-1 --profile typescriptteatime response.json"
    },
    "dependencies": {
        "@googleapis/sheets": "^0.3.0",
        "aws-lambda": "^1.0.6",
        "aws-sdk": "^2.937.0",
        "axios": "^0.21.1",
        "class-validator": "^0.12.2",
        "crypto-js": "^4.0.0",
        "csvtojson": "^2.0.10",
        "expo-server-sdk": "^3.6.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.6.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.77",
        "@types/crypto-js": "^4.0.1",
        "@types/csvtojson": "^2.0.0",
        "@types/faker": "^5.5.6",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@types/node": "12.x",
        "@types/nodemailer": "^6.4.2",
        "@types/uuid": "^8.3.1",
        "del-cli": "^3.0.1",
        "dotenv": "^10.0.0",
        "faker": "^5.5.3",
        "jest": "^26.6.3",
        "prettier": "^2.3.1",
        "serverless": "^2.56.0",
        "serverless-iam-roles-per-function": "^3.2.0",
        "serverless-offline": "^7.0.0",
        "serverless-offline-schedule": "^1.0.4",
        "serverless-plugin-aws-alerts": "^1.7.3",
        "serverless-plugin-typescript": "^1.1.9",
        "serverless-s3-sync": "^1.17.1",
        "ts-jest": "^26.5.6",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.10"
    }
}
