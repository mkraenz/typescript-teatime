    "scripts": {
        "build": "tsc",
        "clean": "del-cli dist .build",
        "predeploy:send-push-notif": "npm run predeploy",
        "deploy:send-push-notif": "serverless deploy --config serverless.pre.yml",
        "predeploy": "npm run clean",
        "deploy": "serverless deploy",
        "deploy:fn": "npm run clean && serverless deploy function -f",
        "deploy:test": "serverless offline",
        "deploy:test:verbose": "serverless offline --printOutput",
        "deploy:test:send-push-notif": "serverless offline --config serverless.pre.yml",
        "dev": "npm run deploy:test",
        "test": "docker-compose up -d && jest"
    },
